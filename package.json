{
  "name": "turbo-start-sanity",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:studio": "turbo run build --filter=studio",
    "build:web": "turbo run build --filter=web",
    "dev": "turbo dev",
    "dev:studio": "turbo run dev --filter=studio",
    "dev:web": "turbo run dev --filter=web",
    "lint": "npx ultracite check",
    "format": "npx ultracite fix",
    "check-types": "turbo check-types",
    "check-biome": "turbo check-biome"
  },
  "dependencies": {
    "@workspace/env": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@workspace/typescript-config": "workspace:*",
    "turbo": "^2.7.5",
    "typescript": "catalog:",
    "ultracite": "7.0.11"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=22"
  }
}
